<Window x:Class="Agent.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Agent"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="700">
    <Grid>
        <TextBox x:Name="textBox" Text="{Binding Path=UserName}"  HorizontalAlignment="Left" Height="26" Margin="279,152,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="149"/>
        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="279,205,0,0" VerticalAlignment="Top" Height="23" Width="149"/>

        <Button x:Name="button" Content="Button" Command="{Binding Path=LoginCommand}" CommandParameter="{Binding ElementName=passwordBox}"  HorizontalAlignment="Left" Margin="279,271,0,0" VerticalAlignment="Top" Width="149" Height="32"/>

        <Label x:Name="label" Content="Label" HorizontalAlignment="Left" Margin="207,152,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.36,-0.038"/>
        <Label x:Name="label1" Content="Label" HorizontalAlignment="Left" Margin="206,205,0,0" VerticalAlignment="Top" Height="23" Width="39"/>

    </Grid>
</Window>
